{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "vueIndentScriptAndStyle": false,
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
